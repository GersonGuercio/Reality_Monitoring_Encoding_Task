    languageStyle='LTR',
    depth=0.0);

# Initialize components for Routine "baseline"
baselineClock = core.Clock()
text_12 = visual.TextStim(win=win, name='text_12',
    text='+',
    font='Arial',
    pos=(-0.25, 0), height=0.1, wrapWidth=None, ori=0, 
